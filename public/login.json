{
    "$jason": {
        "head": {
            "title": "login",
            "actions": {
                "$pull": {
                    "type": "$flush",
                    "success": {
                        "type": "$href",
                        "options": {
                            "url": "https://s3-eu-west-1.amazonaws.com/sopmob.mijnesite.be/login.json"
                        }
                    }
                },
                "$foreground": {
                    "trigger": "load"
                },
                "$show": {
                    "trigger": "load"
                },
                "load": {
                    "type": "$render",
                    "success": {
                        "type": "$util.toast",
                        "options": {
                            "text": "Refreshed"
                        }
                    }
                }
            }
        },
        "templates": {
            "body": {
                "sections": [
                    {
                        "items": {
                            "{{#each $jason}}": {
                                "type": "vertical",
                                "style": {
                                    "spacing": "5"
                                },
                                "components": [
                                    {
                                        "type": "label",
                                        "text": "{{commit.author.date}} {{commit.message}}",
                                        "style": {
                                            "font": "HelveticaNeue-Bold",
                                            "color": "#aaaaaa",
                                            "size": "10"
                                        }
                                    }
                                ]
                            }
                        }
                    },
                    {
                        "items": [
                            {
                                "type": "vertical",
                                "style": {
                                    "padding": 30,
                                    "spacing": 20,
                                    "align": "center"
                                },
                                "components": [
                                    {
                                        "type": "label",
                                        "text": "SOP Mob v2",
                                        "style": {
                                            "align": "center",
                                            "font": "HelveticaNeue-Bold",
                                            "size": 50
                                        }
                                    }
                                ]
                            },
                            {
                                "type": "vertical",
                                "style": {
                                    "padding": 30,
                                    "spacing": 20,
                                    "align": "center"
                                },
                                "components": [
                                    {
                                        "type": "label",
                                        "style": {
                                            "align": "center",
                                            "padding": 10,
                                            "color": "#000000",
                                            "font": "HelveticaNeue",
                                            "size": 24
                                        },
                                        "text": "SOP Mob Test",
                                        "href": {
                                            "url": "https://s3-eu-west-1.amazonaws.com/sopmob.mijnesite.be/login.json",
                                            "fresh": true
                                        }
                                    },
                                    {
                                        "type": "label",
                                        "style": {
                                            "align": "center",
                                            "padding": 10,
                                            "color": "#000000",
                                            "font": "HelveticaNeue",
                                            "size": 24
                                        },
                                        "text": "SOP Mob Main",
                                        "href": {
                                            "url": "https://sopmob.mijnesite.be/main.json",
                                            "fresh": true
                                        }
                                    },
                                    {
                                        "type": "label",
                                        "style": {
                                            "align": "center",
                                            "padding": 10,
                                            "color": "#000000",
                                            "font": "HelveticaNeue",
                                            "size": 20
                                        },
                                        "text": "SOP Mob S3",
                                        "href": {
                                            "url": "https://s3-eu-west-1.amazonaws.com/sopmob.mijnesite.be/main.json",
                                            "fresh": true
                                        }
                                    },
                                    {
                                        "type": "label",
                                        "style": {
                                            "align": "center",
                                            "padding": 10,
                                            "color": "#000000",
                                            "font": "HelveticaNeue",
                                            "size": 20
                                        },
                                        "text": "Jasonette Demo's",
                                        "href": {
                                            "url": "https://raw.githubusercontent.com/jasonelle/docs/develop/examples/jasonette/apps/jasonpedia/demo.json",
                                            "fresh": true
                                        }
                                    },
                                    {
                                        "type": "label",
                                        "style": {
                                            "align": "center",
                                            "padding": 10,
                                            "color": "#000000",
                                            "font": "HelveticaNeue",
                                            "size": 20
                                        },
                                        "text": "Demo ToDo",
                                        "href": {
                                            "url": "https://raw.githubusercontent.com/jasonelle/docs/develop/examples/jasonette/apps/todo-app/home.json",
                                            "fresh": true
                                        }
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
        }
    }
}